{
  "name": "react-simple-inputs",
  "version": "0.0.8",
  "description": "A set of React Input Components for ease of use",
  "main": "lib/index.js",
  "scripts": {
    "start": "babel src -w -d lib",
    "build": "babel src -d lib",
    "test": "jest --env=jsdom",
    "test:watch": "jest --watch --env=jsdom",
    "examples": "parcel ./src/examples/index.html",
    "examples:prod": "parcel build ./src/examples/index.html -d examples"
  },
  "keywords": [
    "react",
    "inputs"
  ],
  "license": "MIT",
  "author": "Patrick Edqvist",
  "homepage": "https://github.com/patrickedqvist/react-simple-inputs",
  "bugs": {
    "url": "https://github.com/patrickedqvist/react-simple-inputs/issues",
    "email": "patrick.edqvist@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/patrickedqvist/react-simple-inputs.git"
  },
  "peerDependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "jest": "^21.2.1",
    "parcel-bundler": "^1.4.1",
    "raf": "^3.4.0"
  }
